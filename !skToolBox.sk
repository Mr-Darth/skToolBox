# Name: Location line
# Description: Gets a line of locations between two points. The density controls the
#              distance between each point.
# Usage: show flame at line from {p1} to {p2} with density 10
plural expression [(all [[of] the]|the)] [(loc[ation]s|points) of] [a] line (between|from|within) %location% (to|and) %location% (with|of|using) [a] density [of] %number%:
	return type: locations
	get:
		set {_vector1} to vector from expr-1
		set {_vector2} to vector from expr-2
		set {_points} to floor({_vector1}.distance({_vector2}) * expr-3) + 1
		set {_d} to (vector from expr-1 to expr-2).multiply(1 / {_points})
		loop {_points} times:
			set {_r::%loop-number%} to (location at 0, 0, 0 in expr-1's world) ~ ({_d}.clone().multiply(loop-number) ++ {_vector1})
		return {_r::*}

# Name: Circle
# Description: Returns the points of a circle from the center, radius, and density. 
#              The density controls the distance between each point.
# Usage: show happy villager at a circle at player with radius 7, density 3
plural expression [(all [[of] the]|the)] [(loc[ation]s|points) of] [a] circle (at|from) %location% (with|of) [a] radius [of] %number%(,| and) [with] [a] density [of] %number%:
	return type: locations
	get:
		set {_points} to floor(expr-2 * expr-3 * 3.141592653589793)
		set {_angle} to 360 / {_points}
		loop {_points} times:
			set {_r::%loop-number%} to expr-1  ~ (new cylindrical vector from expr-2, (loop-number * {_angle}), 0)
		return {_r::*}

# Name: Sphere
# Description: Returns the points of a sphere from its center, radius, and density.
#              The density controls the distance between each point.
# Usage: show smoke at points of a sphere at player with a radius of 10 and a density of 1
plural expression [(all [[of] the]|the)] [(loc[ation]s|points) of] [a] sphere (at|from) %location% (with|of) [a] radius [of] %number%(,| and) [with] [a] density [of] %number%:
	return type: locations
	get:
		set {_points} to floor(expr-2 * expr-3 * 3.141592653589793)
		set {_dy} to 360 / {_points}
		set {_dp} to 360 / {_points}
		loop ({_points} ^ 2) times:
			set {_s} to loop-number
			set {_r::%{_s}%} to expr-1 ~ (new spherical vector expr-2, {_y}, {_p})
			add {_dp} to {_p}
			floor(mod(loop-number, {_points})) = 0
			add {_dy} to {_y}
			set {_p} to 0
		return {_r::*}

# Name: Helix
# Description: Returns the points of a helix from a location, radius, step, height, and density.
#              The density controls the distance between each point.
#              A negative slope will reverse the rotation of the helix.
# Usage: show green spark at helix at player with radius 3, height 10, step 3, density 5
plural expression [(all [[of] the]|the)] [(loc[ation]s|points) of] [a] helix (at|from) %location% (of|with) [a] radius [of] %number%(,| and) [with] [a] height [of] %number%(,| and) [with] [a] [height] (step|increase|slope) [of] %number%(,| and) [with] [a] density [of] %number%:
	return type: locations
	get:
		set {_points} to floor(expr-2 * expr-3 * expr-5 * 2 * 3.141592653589793)
		set {_da} to (360 * expr-3 / (expr-4 * {_points}))
		set {_dh} to expr-3 / {_points}
		loop {_points} times:
			set {_r::%loop-number%} to expr-1 ~ (cylindrical vector with expr-2, ({_da} * loop-number), ({_dh} * loop-number))
		return {_r::*}

# Name: Linked locations
# Description: Returns a list of points that link multiple locations with lines.
#              The density controls the distance between each point.
# Usage: show flame at {_points::*} linked using a density of 10
plural expression:
	return type: locations
	patterns:
		%locations% (linked|connected) (with|using) [a] density [of] %number%
		(link|connect)[ed] %locations% (with|using) [a] density [of] %number%
	get:
		loop exprs-1:
			add 1 to {_i}
			set {_l} to loop-expression
			set {_v::%{_i}%} to vector from {_l}
		loop {_v::*}:
			loop {_v::*}:
				set {_points} to floor((loop-value-1.distance(loop-value-2) * expr-2) + 1)
				set {_d} to (loop-value-2 -- loop-value-1).multiply(1 / {_points})
				loop {_points} times:
					set {_o} to (location at 0, 0, 0 in (first element of exprs-1)'s world) ~ ({_d}.clone().multiply(loop-number) ++ loop-value-1)
					set {_r::%{_o}%} to {_o} if {_r::%{_o}%} is not set
		return {_r::*}

# Name: Triangle
# Description: Returns the points of a triangle from the center, radius, and density
#              The density controls the distance between each point.
# Usage: show happy villager at triangle at player with radius 3 and density 1
# Note: Requires the `Linked locations` expression.
plural expression [(all [[of] the]|the)] [(loc[ation]s|points) of] [a] triangle (at|from) %location% (with|of) [a] radius [of] %number%(,| and) [with] [a] density [of] %number%:
	return type: locations
	get:
		loop 3 times:
			set {_r::%loop-number%} to expr-1  ~ (cylindrical vector from expr-2, (120 * loop-number), 0)
		return link {_r::*} with density expr-3

# Name: Star
# Description: Returns the points of the outline of a star from the center, radius, and density.
#              The density controls the distance between each point.
# Usage: show happy villager at star at block 8 in front of player with radius 10 and density 5
plural expression [(all [[of] the]|the)] [(loc[ation]s|points) of] [a] star (at|from) %location% (with|of) [a] radius [of] %number%(,| and) [a] density [of] %number%:
	return type: locations
	get:
		loop 5 times:
			set {_p::%loop-number%} to expr-1 ~ (cylindrical vector expr-2, (loop-number * 72), 0)
		set {_l::*} to (3, 4, 4, 5 and 5)
		loop {_l::*}:
			set {_i} to (loop-index parsed as number)th element of (1, 1, 2, 2 and 3)
			set {_r::*} to {_r::*} and line between {_p::%{_i}%} and {_p::%loop-value%} with density expr-3
		return {_r::*}

